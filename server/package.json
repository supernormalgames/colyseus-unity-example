{
  "name": "colyseus_server",
  "version": "0.1.0",
  "description": "A general purpose Colyseus server with example two player game",
  "main": "lib/index.js",
  "scripts": {
    "dev": "ts-node-dev index.ts --exit-child --trace-warnings",
    "start": "ts-node index.ts",
    "build": "tsc -p tsconfig.json",
    "debug": "node debug -r ts-node/register index.ts",
    "debug:monitor": "nodemon --inspect index.ts"
  },
  "author": "James Jacoby",
  "license": "MIT",
  "homepage": "https://github.com/supernormalgames/colyseus-unity-example",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.42",
    "nodemon": "^1.19.1",
    "prettier": "2.1.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.14.0",
    "@colyseus/schema": "^1.0.9",
    "@gamestdio/mathf": "^1.0.0",
    "colyseus": "^0.14.6",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "ip": "^1.1.5",
    "nanoid": "^3.1.20"
  }
}
